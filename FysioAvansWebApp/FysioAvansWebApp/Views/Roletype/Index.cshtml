@model IEnumerable<Roletype>
@{
    ViewData["Title"] = "Index";
}

<div class="row">
    <div class="col-lg-12">
        @Html.ActionLink("Create new test", "Create", "Roletype", null, new { @class = "btn btn-success color-green text-dark" })
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped table-bordered">
            <thead>
                <tr>
                    <td>Physiotherapist id</td>
                    <td>Physiotherapist number</td>
                    <td></td>
                </tr>
            </thead>
            <tbody>
                @if (Model.Count() > 0)
                {
                    @foreach (var roletype in Model)
                    {
                        <tr>
                            <td>@roletype.Id</td>
                            <td>@roletype.RoletypeName</td>
                            <td class="text-center">@Html.ActionLink("Edit", "Edit", "Roletype", new { Id = roletype.Id }) | @Html.ActionLink("Delete", "Delete", "Roletype", new { Id = roletype.Id })</td>
                        </tr>
                    }
                }
                else
                {
                <td colspan="4" class="text-center">
                    No records found
                </td>
            }
                </tbody>
            </table>
        </div>
    </div>

